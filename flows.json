    {
        "id": "f515a6706e07e594",
        "type": "function",
        "z": "9ffaf4a50f363f22",
        "name": "spectrumRGB -> [r,g,b]",
        "func": "let colorInt = msg.payload.params.color.spectrumRGB;\n\n// node.warn(\"colorInt=\"+colorInt);\n// node.warn(colorInt);\n\n// Parse the hex string into three separate values for red, green, and blue\nlet r = (colorInt >> 16) & 255;\nlet g = (colorInt >> 8) & 255;\nlet b = colorInt & 255;\n\n// save RGB values as an array\nmsg.colorArray = [r, g, b];\n\n\n\nreturn msg;",
        "outputs": 1,
        "timeout": 0,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
